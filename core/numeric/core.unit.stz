unit = (
  #traits: (numeric_t,)

  #scope: private
  v: fraction )

base-unit = (
  @id: (id: uint,)
  @moniker: (moniker: string,) )

metre   = base-unit id: 2  moniker: 'm'
second  = base-unit id: 3  moniker: 's'
gram    = base-unit id: 5  moniker: 'g'
kelvin  = base-unit id: 7  moniker: 'L'
ampere  = base-unit id: 11 moniker: 'A'
mole    = base-unit id: 13 moniker: 'mol'
candela = base-unit id: 17 moniker: 'cd'
byte    = base-unit id: 19 moniker: 'b'
